{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Photo Album - Index Photos",
  "Resources": {
    "indexphotos": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": "codebucket-ps3202",
          "S3Key": "index_photos.zip"
        },
        "Description": "lambda fn created by cloudformation",
        "Tags": [
          {
            "Value": "SAM",
            "Key": "lambda:createdBy"
          }
        ],
        "MemorySize": 256,
        "Environment": {
          "Variables": {
            "REGION": "us-east-1"
          }
        },
        "Handler": "lambda_function.lambda_handler",
        "Role": "arn:aws:lambda:us-east-1:914919466727:function:index-photos",
        "Timeout": 30,
        "Runtime": "python3.7",
        "FunctionName": "index-photos-cft"
      }
    }
  }
}
